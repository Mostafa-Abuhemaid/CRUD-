@model Department
@{
    ViewData["Title"] = "Details";
}

<h1>Details @Model.Id</h1>
<br />
<form asp-action="Create">
    <div class="form-group">
        <label class="col-form-label">Name</label>
        <input class="form-control" asp-for="Name" readonly />
    
    </div>
    <div class="form-group">
        <label class="col-form-label">Code</label>
        <input class="form-control" asp-for="Code" readonly />

    </div>
    <div class="form-group">
        <label class="col-form-label">datetime</label>
        <input class="form-control" asp-for="datetime" readonly />

    </div>
    <br />
    <a asp-action="Update" class="btn btn-success"> Update</a>
    <a asp-action="Index" class="btn btn-light">back</a>
</form>

