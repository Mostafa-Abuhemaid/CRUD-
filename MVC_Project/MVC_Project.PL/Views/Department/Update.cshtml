@model Department
@{
    ViewData["Title"] = "Update";
}

<h1>Update @Model.Id</h1>

<form asp-action="Update">
    <div class="form-group">
        <label class="col-form-label">Name</label>
        <input class="form-control" asp-for="Name" />
        <span asp-validation-for="Name" class="text-danger "></span>
    </div>
    <div class="form-group">
        <label class="col-form-label">Code</label>
        <input class="form-control" asp-for="Code" />
        <span asp-validation-for="Code" class="text-danger "></span>
    </div>
    <div class="form-group">
        <label class="col-form-label">datetime</label>
        <input class="form-control" asp-for="datetime" />
        <span asp-validation-for="datetime" class="text-danger "></span>
    </div>
    <input type="sumbit" value="save" class="btn btn-primary" />
    <a asp-action="Index" class="btn btn-light">back</a>
</form>
